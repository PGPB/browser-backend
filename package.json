{
    "name": "browser-backend",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "NODE_ENV=production node dist/server",
        "build": "tsc",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-server\"",
        "watch-ts": "tsc --watch",
        "watch-server": "NODE_ENV=development nodemon dist/server"
    },
    "homepage": "https://github.com/PGPB/browser-backend/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/PGPB/browser-backend.git"
    },
    "bugs": {
        "url": "https://github.com/PGPB/browser-backend/issues"
    },
    "author": {
        "name": "PGPB",
        "url": "https://github.com/PGPB"
    },
    "private": true,
    "engines": {
        "node": ">=12.13.0",
        "yarn": ">=1.19.0"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^3.21.2",
        "lusca": "^1.6.1",
        "morgan": "^1.9.1",
        "typescript": "^3.7.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/compression": "^1.0.1",
        "@types/express": "^4.17.2",
        "@types/helmet": "^0.0.45",
        "@types/lusca": "^1.6.1",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.12.6",
        "concurrently": "^5.0.0",
        "nodemon": "^1.19.4"
    }
}
